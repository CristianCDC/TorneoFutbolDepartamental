@page
@model TorneoFutbolDepartamental.App.Frontend.pages.Municipios.IndexModel
@{
}
<h1>Listado de Municipios</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
        </tr>   
    </thead>
    <tbody>
        @foreach (var municipio in Model.municipios)
        {
            <tr>
                <td>@municipio.Nombre</td>
            </tr>
        }
    </tbody>
</table>
